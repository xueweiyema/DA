<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="duties">
        <item>未知</item> <!-- 0 -->
        <item>休息</item> <!-- 1 -->
        <item>通宵</item> <!-- 2 -->
        <item>早班</item> <!-- 3 -->
        <item>中班</item> <!-- 4 -->
        <item>晚班</item> <!-- 5 -->
        <item>早晚</item> <!-- 6 -->
        <item>下通宵</item> <!-- 7 -->

    </string-array>
    <string-array name="stations">
        <item>茂盛围</item>
        <item>横琴</item>
        <item>拱北</item>
    </string-array>
    <string-array name="teams">
        <item>一队</item>
        <item>二队</item>
        <item>三队</item>
        <item>四队</item>
        <item>五队</item>
        <item>六队</item>
        <item>七队</item>
        <item>八队</item>
    </string-array>
    <color name="calendar_active_month_bg">#fff5f7f9</color>
    <color name="calendar_bg">#ffffffff</color>
    <color name="calendar_divider">#ffbababa</color>
    <color name="calendar_highlighted_day_bg">#ccffcc</color>
    <color name="calendar_inactive_month_bg">#ffd7d9db</color>
    <color name="calendar_selected_day_bg">#ff379bff</color>
    <color name="calendar_selected_range_bg">#ff96caff</color>
    <color name="calendar_text_active">#ff778088</color>
    <color name="calendar_text_inactive">#40778088</color>
    <color name="calendar_text_selected">#ffffffff</color>
    <color name="calendar_text_unselectable">#7f778088</color>
    <color name="lightpink">#FFB6C1</color>
    <color name="orange">#FFA500</color>
    <declare-styleable name="CalendarPickerView"><attr name="android:background"/><attr format="color" name="dividerColor"/><attr format="reference" name="dayBackground"/><attr format="color" name="dayTextColor"/><attr format="color" name="titleTextColor"/><attr format="boolean" name="displayHeader"/><attr format="color" name="headerTextColor"/></declare-styleable>
    <declare-styleable name="calendar_cell"><attr format="boolean" name="state_selectable"/><attr format="boolean" name="state_current_month"/><attr format="boolean" name="state_today"/><attr format="boolean" name="state_range_first"/><attr format="boolean" name="state_range_middle"/><attr format="boolean" name="state_range_last"/><attr format="boolean" name="state_highlighted"/></declare-styleable>
    <dimen name="calendar_day_headers_paddingbottom">4dp</dimen>
    <dimen name="calendar_month_title_bottommargin">4dp</dimen>
    <dimen name="calendar_month_topmargin">16dp</dimen>
    <dimen name="calendar_text_medium">18sp</dimen>
    <dimen name="calendar_text_small">14sp</dimen>
    <string name="day_name_format">EEE</string>
    <string name="invalid_date">Date must be between %1$s and %2$s.</string>
    <string name="month_name_format">MMMM yyyy</string>
    <style name="CalendarCell">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">center</item>
    <item name="android:textAlignment">center</item>
  </style>
    <style name="CalendarCell.CalendarDate">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
    <item name="android:clickable">true</item>
    <item name="android:textStyle">bold</item>
  </style>
    <style name="CalendarCell.DayHeader">
    <item name="android:textSize">@dimen/calendar_text_small</item>
  </style>
    <style name="CalendarTitle">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
    <item name="android:textStyle">bold</item>
  </style>
</resources>